n=int(input("Enter an integer number: "))

divisor=1

temp=n

while temp//divisor>=10:

 divisor*=10

while divisor>0:

 digit=temp//divisor

 print(digit,end=" ")

 temp%=divisor

 divisor//=10



num=int(input("Enter an integer number: "))
reversed_num=0
while num!=0:
 digit=num%10
 reversed_num=reversed_num*10+digit
 num//=10
print("Reversed Number: ",reversed_num)
